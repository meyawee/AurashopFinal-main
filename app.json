{
  "expo": {
    "name": "Aurashop",
    "slug": "aurashop-o2yg2lpol-t1yalvbf6t7",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://img.freepik.com/premium-vector/auto-detailing-car-logo-automotive-car-wash-logo-vector-template_852937-3299.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": ".https://img.freepik.com/premium-vector/auto-detailing-car-logo-automotive-car-wash-logo-vector-template_852937-3299.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "ios": {
      "bundleIdentifier": "com.itgenius.aurashop",
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to let you upload profile photos.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to let you upload profile photos."
      }
    },
    "android": {
      "package": "com.itgenius.aurashop",
      "versionCode": 1,
      "ndkVersion": "26.0.10792818",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you share photos with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "014c1808-42f3-47ec-96bc-503982b36a6f"
      }
    },
    "owner": "meyawee"
  }
}
